<%- include('partials/head') %>
  <body>
    <%- include('partials/navbar') %>
    <h1>Your random number is <%= num %></h1>
    <% if(num % 2 === 0){ %>
    <h2>This is an even number!</h2>
    <% } else { %>
    <h2>This is an odd number!</h2>
    <% } %>
    <h3>That number is: <%= num%2===0 ? 'EVEN' : 'ODD' %></h3>
  </body>
</html>
